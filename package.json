{
  "name": "app-api",
  "version": "1.6.2",
  "main": "index.ts",
  "private": true,
  "engines": {},
  "scripts": {
    "apollo:generate": "graphql-codegen --config codegen.yml",
    "build": "NODE_ENV=production rimraf ./build && tsc && yarn copy-files",
    "copy-files": "copyfiles -f ./src/seedData/*.txt ./build/src/seedData",
    "cron:estimated-carbon-intensities": "SUPPRESS_SUBSCRIBERS=1 ts-node src/cronjobs/estimatedCarbonIntensities",
    "cron:save-additional-company-recommendation-data": "SUPPRESS_SUBSCRIBERS=1 ts-node src/cronjobs/saveAdditionalCompanyRecommendationData",
    "dev": "NODE_ENV=development nodemon",
    "hubspot:sync": "ts-node src/scripts/sync-hubspot-resources",
    "hubspot:resendInviteToJoin": "ts-node src/scripts/resendInviteToJoin",
    "lint": "eslint . --ext .ts",
    "lint:fix": "yarn lint --fix",
    "migrate": "ts-node ./node_modules/typeorm/cli.js -f ormconfig.migrations.js migration:run",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js -f ormconfig.migrations.js migration:create -n",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js -f ormconfig.migrations.js migration:revert",
    "predev": "yarn migrate && yarn seed:local",
    "prepare": "husky install",
    "process-queues": "ts-node ./src/jobs/queues/process.ts",
    "seed:local": "node ./localSeed.js",
    "start": "NODE_ENV=production node build/src/index.js",
    "test:unit": "NODE_ENV=test jest -c jest.unit.config.js --maxWorkers=2",
    "test:integration": "NODE_ENV=test jest -c jest.integration.config.js --runInBand",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.24.0",
